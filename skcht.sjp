let xbolinha = 300;
let ybolinha = 200;
let diametro = 22;

let velocidadeXBolinha = 6;
let velocidadeYBolinha = 6;

function setup() {
  createCanvas(600, 400);
}

function draw() {
  background(0);
  circle(xbolinha, ybolinha, diametro);
  xbolinha += velocidadeXBolinha;
  ybolinha += velocidadeYBolinha;
}